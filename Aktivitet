package softwarehuset;

public class Aktivitet {
	
	String aktivitet, Medarbejder;
	int projektNummer, startUge, slutUge, forventedeArbejdsTimer, antalArbejdstimer;
	int forventedeArbejdsTimertemp;
	
	public Aktivitet(String aktivitet, int projektNummer, int startUge, int slutUge, int forventedeArbejdsTimer) {
		this.aktivitet = aktivitet;
		this.projektNummer = projektNummer;
		this.startUge = startUge;
		this.slutUge = slutUge;
		this.forventedeArbejdsTimer = forventedeArbejdsTimer;
		
	}
	
	public String getAktivitet(){
		return aktivitet;
	}
	public String getMedarbejder(){
		return Medarbejder;
	}
	
	public int projektNummer(){
		return projektNummer;
	}
	
	public int getStartUge(){
		return startUge;
	}
	
	public int getsSlutUge(){
		return slutUge;
	}
	
	public int getForventedeArbejdsTimer(){
		forventedeArbejdsTimertemp = forventedeArbejdsTimer;
		forventedeArbejdsTimertemp = forventedeArbejdsTimertemp-antalArbejdstimer;
		return forventedeArbejdsTimertemp;
	}
	
	public int setAntalArbejdstimer(int arbejdstimer){
		antalArbejdstimer = antalArbejdstimer+ arbejdstimer;
		return antalArbejdstimer;
	}
	public int getAntalArbejdsTimer(){
		return antalArbejdstimer;
	}
}
