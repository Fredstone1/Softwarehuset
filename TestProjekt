package softwarehuset;

import static org.junit.Assert.*;

import org.junit.Test;

public class TestProjekt {
	Softwarehuset prog = new Softwarehuset();
	
	@Test
	public void testProjekt() {
		Projekt proj1 = new Projekt("Softwarehuset");
		Projekt proj2 = new Projekt("Magnushuset");
		prog.opretProjekt(proj1);
		prog.opretProjekt(proj2);
		
		//Testning af oprettelse af projekt
		assertEquals(Softwarehuset.ProjektListe.size(),2);
		assertEquals(Softwarehuset.ProjektListe.get(0).Projektnummer,"170001");
		assertEquals(Softwarehuset.ProjektListe.get(0).Navn,"Softwarehuset");
		
		assertEquals(Softwarehuset.ProjektListe.get(1).Projektnummer,"170002");
		assertEquals(Softwarehuset.ProjektListe.get(1).Navn,"Magnushuset");
		
	}

}
